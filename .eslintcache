rtItem={onSelectSortType}\n        />\n      </div>\n      <h2 className=\"content__title\">Все пиццы</h2>\n      <div className=\"content__items\">\n        {isLoaded\n          ? items.map((obj) => (\n              <PizzaBlock\n                addPizzaToCart={addPizzaToCart}\n                addedCountPizzas={\n                  cartItems[obj.id] && cartItems[obj.id].items.length\n                }\n                key={obj.id}\n                {...obj}\n              />\n            ))\n          : Array(10)\n              .fill(0)\n              .map((item, index) => (\n                <PizzaLoadingBlock key={`${item}_${index}`} />\n              ))}\n      </div>\n    </div>\n  );\n};\n\nexport default Home;\n","D:\\projects\\react-pizza\\src\\pages\\index.js",[],"D:\\projects\\react-pizza\\src\\pages\\Cart.jsx",[],"D:\\projects\\react-pizza\\src\\components\\PizzaBlock\\PizzaBlock.jsx",[],"D:\\projects\\react-pizza\\src\\redux\\store.js",[],"D:\\projects\\react-pizza\\src\\redux\\reducers\\index.js",[],"D:\\projects\\react-pizza\\src\\redux\\reducers\\filters.js",[],"D:\\projects\\react-pizza\\src\\redux\\reducers\\pizzas.js",[],"D:\\projects\\react-pizza\\src\\redux\\actions\\pizzas.js",[],"D:\\projects\\react-pizza\\src\\redux\\actions\\filters.js",[],"D:\\projects\\react-pizza\\src\\components\\PizzaLoadingBlock\\PizzaLoadingBlock.jsx",[],"D:\\projects\\react-pizza\\src\\redux\\reducers\\cart.js",[],"D:\\projects\\react-pizza\\src\\redux\\actions\\cart.js",[],"D:\\projects\\react-pizza\\src\\components\\CartItem\\CartItem.jsx",[],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","severity":1,"message":"98","line":36,"column":6,"nodeType":"99","endLine":36,"endColumn":24,"suggestions":"100"},{"ruleId":"97","severity":1,"message":"101","line":40,"column":6,"nodeType":"99","endLine":40,"endColumn":8,"suggestions":"102"},{"ruleId":"97","severity":1,"message":"101","line":44,"column":6,"nodeType":"99","endLine":44,"endColumn":8,"